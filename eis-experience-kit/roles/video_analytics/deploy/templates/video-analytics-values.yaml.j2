---

image: "{{ docker_registry_address }}/{{ docker_name }}:{{ eis_version }}"
dockerRegistryEndpoint: "{{ docker_registry_address }}"
cameraStreamPort: "{{ camera_stream_port }}"
cameraStreamResultsPort: "{{ camera_stream_results_port }}"
devMode: "{{ dev_mode }}"
clientsList: "{{ clients_list }}"
appName: "{{ app_name }}"
publisherPodService: "{{ publisher_pod_service }}"
eisSecretsPath: "{{ eis_secrets_path }}"
certPath: "{{ eis_secrets_path }}/{{ cert_name }}"
keyPath: "{{ eis_secrets_path }}/{{ key_name }}"
rootCACertPath: "{{ eis_secrets_path }}/{{ rootca_cert_name }}"
